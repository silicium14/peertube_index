          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n    CASE\n        WHEN reason ~ '\\{:failed_connect, \\[\\{:to_address, .*'\n            THEN regexp_replace(reason, '(\\{:failed_connect, \\[\\{:to_address, \\{)(''.*'')(.*)', '\\1<hostname>\\3')\n        WHEN reason ~ '%Poison\\.ParseError.*'\n            THEN '%Poison.ParseError'\n        ELSE reason\n    END AS generic_reason,\n    count(host) as count,\n    round(100*count(host)::float/(SELECT count(*) FROM instance WHERE status = 'error')) as \"%\"\nFROM instance\nWHERE status = 'error'\nGROUP BY generic_reason;",
          "refId": "A",
